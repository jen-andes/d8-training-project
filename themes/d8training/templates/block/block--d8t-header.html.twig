{{ attach_library('d8training/header') }}
{% include '@d8training/header/header.twig' with {
    'email': content['#email'],
    'phone': content['#phone'],
    'cta': {
      'url': '/reservation',
      'label': 'Book a table'
    },
    'facebook': content['#social_links'].facebook,
    'twitter': content['#social_links'].twitter,
    'dribble': content['#social_links'].dribble, 
    'behance': content['#social_links'].behance,
    'feed': content['#social_links'].feed,
    'menus': content['#menu'].items
  } only
%}
